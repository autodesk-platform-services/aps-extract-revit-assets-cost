        },
        "APS_WEBHOOK_URL": {
            "description": "Base domain URL of your webhook",
            "value": "http://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com"
        }
    },
    "website": "https://developer.autodesk.com/",
    "success_url": "/"
}